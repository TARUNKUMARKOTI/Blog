<%- include("partials/header")%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="mb-3">
        <label for="title" class="form-label">Title</label>
        <input type="text" class="form-control" name="postTitle" id="title">
    </div>
    <div class="mb-3">
        <label for="post" class="form-label">Post</label>
        <textarea class="form-control" id="post" name="postBody" rows="3"></textarea>
    </div>
    <button style="margin-top: 10px;" class="btn btn-primary" type="submit" name="compose">Publish</button>
    
</form>
<%- include("partials/footer")%>